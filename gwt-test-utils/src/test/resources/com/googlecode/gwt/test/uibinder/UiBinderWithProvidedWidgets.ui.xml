<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:test='urn:import:com.googlecode.gwt.test.uibinder'>

    <g:HTMLPanel>
        <test:ProvidedLabel
                ui:field="firstProvidedLabel"
                customText="first custom text setup in ui.xml">my first label
        </test:ProvidedLabel>
        <test:ProvidedLabel
                ui:field="secondProvidedLabel"
                customText="second custom text setup in ui.xml">my second label
        </test:ProvidedLabel>
    </g:HTMLPanel>

</ui:UiBinder>